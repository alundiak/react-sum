{
  "name": "@lundiak/react-sum",
  "version": "1.4.1",
  "description": "Experimental ReactJS component to show sum of 2 numbers.",
  "private": false,
  "type": "module",
  "main": "dist/App.js",
  "types": "types/App.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build:types": "tsc --project tsconfig.dist.json",
    "build:js": "vite build",
    "build": "npm run build:types && npm run build:js",
    "lint": "eslint ./src",
    "test": "vitest",
    "preview": "vite preview",
    "ladle": "ladle serve",
    "ladle:build": "ladle build",
    "ladle:preview": "ladle preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "ascii-math": "^2.0.0",
    "asciimath-parser": "^0.6.5",
    "katex": "^0.16.22",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@amiceli/vitest-cucumber": "^5.1.2",
    "@eslint/js": "^9.33.0",
    "@ladle/react": "^5.0.3",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alundiak/react-sum.git"
  },
  "bugs": {
    "url": "https://github.com/alundiak/react-sum/issues"
